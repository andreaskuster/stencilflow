{
	"inputs": {
		"wtgfac": [1, 1, 1, 1, 1, 1, 1, 1]
	},
	"outputs": ["div"],
	"dimensions": [100, 100, 100],
	"program": {
		"vdc": "res = 0.25 * (wtgfac[i,j,k+1] + wtgfac[i,j,k-1] + wtgfac[i,j+1,k] + wtgfac[i,j-1,k]) + vout[i,j,k]",

		"ppgk": "res = 0.25 * (wtgfac[i,j,k+1] + wtgfac[i,j,k-1] + wtgfac[i,j+1,k] + wtgfac[i,j-1,k])",

        "ppgc": "res = 0.25 * (ppgk[i,j,k+1] + ppgk[i,j,k-1] + ppgk[i,j+1,k] + ppgk[i,j-1,k])",

        "ppgv": "res = 0.25 * (ppgc[i,j,k+1] + ppgc[i,j,k-1] + ppgc[i,j+1,k] + ppgc[i,j-1,k])",

        "vout": "res = 0.25 * (ppgv[i,j,k+1] + ppgv[i,j,k-1] + ppgv[i,j+1,k] + ppgv[i,j-1,k])",

        "div": "OUT = vdc[i,j,k] + vout[i,j,k]; res = OUT if ppgk[i,j,k] < wtgfac[i,j,k] else wtgfac[i,j,k];"
	}
}