{
	"inputs": {
		"a": {
			"data": "stencils/inC.dat",
			"data_type": "float64"
		}
	},
	"outputs": ["b"],
	"dimensions": [16, 16, 16],
	"program": {
		"b": {
			"computation_string": "res = 0.16666666 * (a[i-1,j,k] + a[i+1,j,k] + a[i,j-1,k] + a[i,j+1,k] + a[i,j,k-1] + a[i,j,k+1])",
			"boundary_condition": {
				"a": {
					"type": "constant",
					"value": 0.0
				}
			},
			"data_type": "float64"
		}
	}
