{
	"inputs": {
		"arrA": {
			"data": "stencils/data/zeros_16x16x16_fp64.dat",
			"data_type": "float64"
		}
	},
	"outputs": ["res"],
	"dimensions": [16, 16, 16],
	"program": {
		"kA": {
			"computation_string": "res = arrA[i,j,k] + 1.0",
			"boundary_condition": {
				"arrA": {
					"type": "constant",
					"value": 0.0
				}
			},
			"data_type": "float64"
		},
      "kB": {
			"computation_string": "res = kA[i,j,k-100] + kA[i,j,k+2]",
			"boundary_condition": {
				"kA": {
					"type": "constant",
					"value": 0.0
				}
			},
			"data_type": "float64"
		},
      "res": {
			"computation_string": "res = kA[i,j,k] + kB[i,j,k]",
			"boundary_condition": {
				"kA": {
					"type": "constant",
					"value": 0.0
				},
              "kB": {
					"type": "constant",
					"value": 0.0
				}
			},
			"data_type": "float64"
		}
	}
}