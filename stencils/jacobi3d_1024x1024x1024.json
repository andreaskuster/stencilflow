{
	"inputs": {
		"a": {
			"data": "stencils/data/zeros_1024x1024x1024_fp32.dat",
			"data_type": "float32"
		}
	},
	"outputs": ["b"],
	"dimensions": [1024, 1024, 1024],
	"program": {
		"b": {
			"computation_string": "res = 0.16666666 * (a[i-1,j,k] + a[i+1,j,k] + a[i,j-1,k] + a[i,j+1,k] + a[i,j,k-1] + a[i,j,k+1])",
			"boundary_conditions": {
				"a": {
					"type": "constant",
					"value": 1.0
				}
			},
			"data_type": "float32"
		}
	}
}
