{
	"inputs": {
		"arrA": {
			"data": "stencils/inA.csv",
			"data_type": "float64"
		}
	},
	"outputs": ["res"],
	"dimensions": [10, 10, 10],
	"program": {
		"res": {
			"computation_string": "res = (((arrA[i,j,k] + arrA[i,j,k-1]) + arrA[i,j+1,k+1]) + arrA[i+1,j+1,k+1]) + 3.14",
			"boundary_conditions": {
				"arrA": {
					"type": "constant",
					"value": 0.0
				}
			},
			"data_type": "float64"
		}
	}
}