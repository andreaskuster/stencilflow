			"computation_string": "res = kA[i,j,k-100] + kA[i,j,k+2]",
			"boundary_conditions": {
				"kA": {
					"type": "constant",
					"value": 0.0
				}
			},
			"data_type": "float64"
		},
      "res": {
			"computation_string": "res = kA[i,j,k] + kB[i,j,k]",
			"boundary_conditions": {
				"kA": {
					"type": "constant",
					"value": 0.0
				},
              "kB": {
